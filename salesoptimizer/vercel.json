{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "../../backend/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}