{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "salesoptimizer/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "salesoptimizer/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "salesoptimizer/$1"
    }
  ]
}